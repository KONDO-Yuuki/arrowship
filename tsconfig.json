{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["esnext"],
    "moduleResolution": "Node",
    "noUnusedLocals": true,
    "strict": true,
    "strictFunctionTypes": false
  }
}
